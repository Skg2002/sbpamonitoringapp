{
  "_version": "1.65.0",
  "sap.app": {
    "id": "automationhealthapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.5",
      "toolsId": "5cf4b767-6aa8-4e2b-ba19-f44602720aaa"
    },
    "dataSources": {
    "wfService": {
        "uri": "/odata/v4/external/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.128.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.viz": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "automationhealthapp.i18n.i18n"
        }
      },
      "wfModel":{
          "dataSource": "wfService",
            "preload": true,
            "settings": {
              "operationMode": "Server",
              "autoExpandSelect": true,
              "earlyRequests": true
        }
      },
      "appModel":{
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "automationhealthapp.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Routeautomationhealthview",
          "pattern": ":?query:",
          "target": [
            "Targetautomationhealthview"
          ]
        },
        {
          "name": "selectedprocessdetails",
          "pattern": "Details/{Tenant},{IDs}",
          "target": [
            "TargetSelectedProcessDetailsview"
          ]
        }
      ],
      "targets": {
        "Targetautomationhealthview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "automationhealthview",
          "viewName": "automationhealthview"
        },
        "TargetSelectedProcessDetailsview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "selectedprocessdetails",
          "viewName": "selectedprocessdetails"
        }
      }
    },
    "rootView": {
      "viewName": "automationhealthapp.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "approuter"
  }
}
